{
  "name": "@theutkarshdev/rapid-api-kit",
  "version": "1.2.0",
  "description": "Zero-config REST API generator with file uploads. Give your MongoDB creds + schema, get full CRUD endpoints with Swagger docs and Vercel Blob file uploads instantly.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "example:js": "node example/javascript/index.js",
    "example:ts": "tsx example/typescript/index.ts",
    "dev": "tsx example/typescript/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rest-api",
    "api-generator",
    "express",
    "mongoose",
    "swagger",
    "crud",
    "backend",
    "rapid-api",
    "auto-api",
    "frontend-students",
    "mock-api",
    "api-scaffold",
    "file-upload",
    "vercel-blob",
    "multipart"
  ],
  "author": "rapid-api <theutkarshdev>",
  "license": "MIT",
  "dependencies": {
    "@vercel/blob": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/swagger-ui-express": "^4.1.6",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theutkarshdev/rapid-api-kit.git"
  },
  "homepage": "https://github.com/theutkarshdev/rapid-api-kit#readme",
  "bugs": {
    "url": "https://github.com/theutkarshdev/rapid-api-kit/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
